{
  "name": "tree-sitter-godoc",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Go documentation (godoc) output",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "godoc",
    "go",
    "documentation"
  ],
  "author": "Fredrik Averpil",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp": "^10.0.1",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "tree-sitter": [
    {
      "scope": "source.godoc",
      "injection-regex": "^godoc$",
      "file-types": [
        "godoc"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/**",
    "queries/*",
    "src/**"
  ],
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fredrikaverpil/tree-sitter-godoc"
  }
}
